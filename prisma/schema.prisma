generator client {
  provider = "prisma-client-js"
  output   = "./generated/prisma"
  
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Ticket {
    id       String    @id @default(uuid())
    content  String 
    priority String 
    status   String
}

model Faq {
  id             String @id @default(uuid())
  question       String
  answer         String
}
